<div class="container-fluid">
  <h1>Show Project</h1>
  
  <p>
    <strong>Title:</strong>
    <%= @project.title %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= @project.description %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= @project.price %>
  </p>

  <%= link_to 'Edit', edit_project_path(@project), class: "btn btn-link" %> |
  <%= link_to 'Back', projects_path, class: "btn btn-link" %>
  
</div>
